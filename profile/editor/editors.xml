<!-- Version 0.0.2 -->
<!-- Ref: units of measure from https://wiki.universal-devices.com/index.php?title=ISY_Developers:API:V5:Appendix:Units_of_Measure -->
<editors>
  <editor id="BOOLEAN">
    <!-- 2 is boolean -->
    <range uom="2" subset="0-1"/>
  </editor>
  <editor id="I_AMP">
    <!-- 1 is Ampere -->
    <range uom="1" subset="0-100000"/>
  </editor>
  <editor id="I_VOLT">
    <!-- 72 is volt -->
    <range uom="72" subset="0-1000"/>
  </editor>
  <editor id="I_WATT">
    <!-- 73 is watt -->
    <range uom="73" subset="0-100000"/>
  </editor>
  <editor id="I_MILES">
    <!-- 56 is raw value as reported by the device -->
    <range uom="56" subset="0-1000"/>
  </editor>
  <editor id="I_ODO">
    <!-- 56 is raw value as reported by the device -->
    <range uom="56" min="0" max="1000000" step="100" prec="0"/>
  </editor>
  <editor id="I_RAW">
    <!-- 56 is raw value as reported by the device -->
    <range uom="56" subset="0"/>
  </editor>
  <editor id="I_PERCENT">
    <!-- 51 is percent -->
    <range uom="51" subset="0-100" />
  </editor>
  <editor id="I_TEMP_DEG">
    <!-- 4 is celsius (C) -->
    <range uom="4" min="-40" max="60" step="1" prec="0" />
  </editor>
  <editor id="I_HEATED_SEAT_LEVEL_RANGE">
    <!-- 0 is off 4 is full heat -->
    <range uom="56" min="0" max="3" step="1" prec="0" />
  </editor>
  <editor id="I_CLIMATE_TEMP_RANGE">
    <!-- 15 to 28 degrees C -->
    <range uom="56" min="15" max="28" step="1" prec="0" />
  </editor>
</editors>
