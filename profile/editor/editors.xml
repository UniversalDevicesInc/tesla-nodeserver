<!-- Version 0.0.2 -->
<!-- Ref: units of measure from https://wiki.universal-devices.com/index.php?title=ISY_Developers:API:V5:Appendix:Units_of_Measure -->
<editors>
  <editor id="BOOLEAN">
    <!-- 2 is boolean -->
    <range uom="2" subset="0-1"/>
  </editor>
  <editor id="ONOFF">
    <!-- 78 is off/on -->
    <range uom="78" />
  </editor>
  <editor id="I_AMP">
    <!-- 1 is Ampere -->
    <range uom="1" subset="0-100000"/>
  </editor>
  <editor id="I_VOLT">
    <!-- 72 is volt -->
    <range uom="72" subset="0-1000"/>
  </editor>
  <editor id="I_WATT">
    <!-- 73 is watt -->
    <range uom="73" subset="0-100000"/>
  </editor>
  <editor id="I_BATTERY_RANGE">
    <!-- uom is gathered from the vehicle -->
    <range uom="116" subset="0-1000"/>
    <range uom="83" subset="0-1609"/>
  </editor>
  <editor id="I_ODO">
    <!-- uom is gathered from the vehicle -->
    <range uom="116" min="0" max="1000000" step="100" prec="0"/>
    <range uom="83"  min="0" max="1609344" step="100" prec="0"/>
  </editor>
  <editor id="I_RAW">
    <!-- 56 is raw value as reported by the device -->
    <range uom="56" subset="0"/>
  </editor>
  <editor id="I_PERCENT">
    <!-- 51 is percent -->
    <range uom="51" subset="0-100" />
  </editor>
  <editor id="I_TEMP_DEG">
    <!-- 4 is celsius (C) -->
    <range uom="4" min="-40" max="60" step="1" prec="0" />
    <range uom="17" min="-40" max="140" step="1" prec="0" />
  </editor>
  <editor id="I_SUNROOF">
    <!-- 0 not available -->
    <range uom="25" subset="101" nls="EN_I_SUNROOF" />
    <range uom="51" subset="0-100" />
  </editor>
  <editor id="I_HEATED_SEAT_LEVEL_RANGE">
    <!-- 0 is off 4 is full heat -->
    <range uom="25" subset="0-3" nls="EN_I_SEAT_HEATER_LEVEL" />
  </editor>
  <editor id="I_CHARGING_STATE">
    <!-- 0 is Stopped, 1 is Disconnected, 2 is Charging, 3 is Complete -->
    <range uom="25" subset="0-3" nls="EN_I_CHARGING_STATE" />
  </editor>
  <editor id="I_VEHICLE_STATE">
    <!-- 0 is asleep, 1 is online -->
    <range uom="25" subset="0-2" nls="EN_I_VEHICLE_STATE" />
  </editor>
  <editor id="I_CLIMATE_TEMP_RANGE">
    <!-- 15 to 28 degrees C -->
    <range uom="4" min="15" max="28" step="1" prec="0" />
    <range uom="17" min="60" max="82" step="1" prec="0" />
  </editor>
  <editor id="I_CLIMATE_TEMP_RANGE_C">
    <!-- 15 to 28 degrees C -->
    <range uom="4" min="15" max="28" step="1" prec="0" />
  </editor>
  <editor id="I_CLIMATE_TEMP_RANGE_F">
    <!-- 60 to 82 degrees F -->
    <range uom="17" min="60" max="82" step="1" prec="0" />
  </editor>
  <editor id="I_SOFTWARE_UPDATE_STATUS">
    <range uom="25" subset="0-3" nls="EN_I_SOFTWARE_UPDATE_STATUS" />
  </editor>
  <editor id="I_OPEN_STATUS">
    <range uom="25" subset="0-1" nls="EN_I_OPEN_STATUS" />
  </editor>
  <editor id="I_SENTRY_MODE">
    <range uom="25" subset="0-2" nls="EN_I_SENTRY_MODE" />
  </editor>
  <editor id="I_CHARGE_PORT_DOOR">
    <range uom="25" subset="0-1" nls="EN_I_CHARGE_PORT_DOOR" />
  </editor>
  <editor id="I_SECURITY_ENABLED">
    <range uom="25" subset="0-1" nls="EN_I_SECURITY_ENABLED" />
  </editor>

</editors>
